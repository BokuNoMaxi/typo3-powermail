{
  "name": "in2code/invisiblerecaptcha",
  "description": "Google invisible recaptcha extension for powermail",
  "keywords": [
    "typo3",
    "powermail",
    "captcha",
    "recaptcha",
    "invisiblerecaptcha"
  ],
  "homepage": "https://github.com/einpraegsam/invisiblerecaptcha",
  "authors": [
    {
      "name": "Alex Kellner",
      "email": "alexander.kellner@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms": ">=8.7.0 <10.0.0",
    "in2code/powermail": ">=5.0.0 <6.0.0"
  },
  "replace": {
    "invisiblerecaptcha": "self.version",
    "typo3-ter/invisiblerecaptcha": "self.version"
  },
  "autoload": {
    "psr-4": {
      "In2code\\Invisiblerecaptcha\\": "Classes"
    }
  }
}
